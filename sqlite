# use sqlite interactively
sqlite3

# open database
.open FILENAME

# show database
.databases

# show schema
.schema

# show table structure
.schema table_name

# dump content
.dump

# save output as csv file
.mode csv
.output filename.csv
<run query>

# save as tsv file
.mode tabs
.output filename.tsv
<run query>

# quit or exit
.quit
